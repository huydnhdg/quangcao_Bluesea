    <style>
        * {
            margin: 0;
            padding: 0;
        }

        body {
            background: #e0e0e0;
        }

        #totop, #totop2 {
            position: fixed;
            right: 0;
            bottom: 80px;
            width: 80px;
            height: 80px;
            overflow: hidden;
            cursor: pointer;
            display: none;
            background: url("/Assets/client/img/gototop.png") no-repeat 0px 0;
            z-index: 9999;
        }

        #totop2 {
            background: url("/Assets/client/img/gototop.png") no-repeat -149px 0;
        }
    </style>
    <script>
        $(function () {
            $('html').append('<div id="totop"></div><div id="totop2"></div>')
            $('#totop').hover(function () { $('#totop2').fadeIn().mouseout(function () { $(this).fadeOut(200) }) })
            var defaultTop = $(window).height() - 10 - 80//10 là vị trí cạnh bottom ban đầu, 249 là chiều cao
            $(window).resize(function () {
                defaultTop = $(window).height() - 10 - 80//phòng trường hợp resize trình duyệt
            })
            var ranPo = ['-298px 0', '-447px 0', '-596px 0', '-745px 0']
            $(window).scroll(function () {
                var $top = $(window).scrollTop()
                if ($top > 50) {
                    $('#totop').fadeIn(100)
                    $('#totop2').css('top', defaultTop)
                }
                else {
                    $('#totop,#totop2').fadeOut(100)
                }
            })
            $('#totop2').click(function () {
                timeranPo = window.setInterval(function () {
                    $('#totop2').css({ 'background-position': ranPo[Math.floor(Math.random() * ranPo.length)], 'display': 'block' })
                }, 200)
                setTimeout(function () {
                    $('html,body').stop().animate({
                        scrollTop: '50px'
                    }, 400, function () {
                        $('#totop2').stop().animate({ 'top': '-250px' }, 300, function () {
                            clearInterval(timeranPo)
                            $('#totop2').css('background-position', '-149px 0').hide()
                        })
                    })
                }, 800)
            })
        })
    </script>

