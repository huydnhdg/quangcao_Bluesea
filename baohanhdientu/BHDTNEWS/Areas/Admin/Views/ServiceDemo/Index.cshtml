@model IEnumerable<BHDTNEWS.Models.ServiceDemo>
@{
    ViewBag.Title = "Index";
}
    <div class="container-fluid">
        <div class="card shadow mb-4">
            <div class="card-header py-3">
                <h6 class="m-0 font-weight-bold text-primary">Danh sách dùng thử dịch vụ</h6>
            </div>
            <div class="card-body">
                <div class="table-responsive">
                    <table class="table table-bordered" id="dataTable" width="100%" cellspacing="0">
                        <thead>
                            <tr>
                                <th>Company Name</th>
                                <th>Product Name</th>
                                <th>Hotline</th>
                                <th>Product Code</th>
                                <th>Time Warranti</th>
                            </tr>
                        </thead>
                        <tfoot>
                            <tr>
                                <th>Com Name</th>
                                <th>Product Name</th>
                                <th>Hotline</th>
                                <th>Product Code</th>
                                <th>Time Warranti</th>
                                <th></th>
                            </tr>
                        </tfoot>
                        <tbody>
                            @foreach (var item in Model)
                            {
                            <tr>
                                <th>@item.ComName</th>
                                <th>@item.ProdName</th>
                                <th>@item.Hotline</th>
                                <th>@item.ProdCode</th>
                                <th>@item.TimeWarranti</th>
                                <td>
                                    <a class="btn" href="/admin/servicedemo/edit/@item.Id"><i class="fa fa-edit"></i></a>
                                    <a class="btn" href="/admin/servicedemo/delete/@item.Id"><i class="fa fa-trash"></i></a>
                                </td>
                            </tr>
                            }
                        </tbody>
                    </table>
                </div>
            </div>
        </div>

    </div>

