@model IEnumerable<EINVOICE_BLUESEA.Models.Category>
@{
    ViewBag.Title = "Index";
}

    <div class="card">
        <div class="card-header">
            <h3 class="card-title">DataTable with default features</h3>
        </div>
        <!-- /.card-header -->
        <div class="card-body">
            <table id="example1" class="table table-bordered table-striped">
                <thead>
                    <tr>
                        <th>Phân loại bài viết</th>
                        <th>Ngày tạo</th>
                        <th></th>
                    </tr>
                </thead>
                <tbody>
                    @foreach (var item in Model)
                    {
                        <tr>
                            <td>@item.Title</td>
                            <td>
                                @item.Createdate
                            </td>
                            <td>
                                <a class="btn" href="/admin/categorynews/edit/@item.Id"><i class="fa fa-edit fa-fw"></i></a>
                                <a class="btn" href="/admin/categorynews/delete/@item.Id"><i class="fa fa-trash"></i></a>
                            </td>
                        </tr>
                    }

                </tbody>

            </table>
        </div>
        <!-- /.card-body -->
    </div>

